<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="gridPane" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" percentWidth="5.0" />
    <ColumnConstraints hgrow="SOMETIMES" percentWidth="55.0" />
    <ColumnConstraints hgrow="SOMETIMES" percentWidth="35.0" />
      <ColumnConstraints hgrow="SOMETIMES" percentWidth="5.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints percentHeight="100.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label fx:id="nombreLabel" styleClass="texto-secundario" text="Nombre" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="dniLabel" styleClass="texto-secundario" text="Dni" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</GridPane>
